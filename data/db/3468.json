    "recipeIngredient": [
      "1 ч.ч. брашно",
      "1 ч.ч. захар",
      "3  яйца",
      "1/2 ч.ч. олио",
      "1/2 ч.л. бакпулвер",
      "1 опак. ванилия",
      "1  банан",
