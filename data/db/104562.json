    "recipeIngredient": [
      "300 г агнешко месо",
      "1/2 ч.ч. булгур",
      "3 стръка пресен лук",
      "1 кубче пилешки бульон",
      "5 моркова",
      "5 стръка керевиз",
      "5 стръка магданоз",
      "50 г масло",
