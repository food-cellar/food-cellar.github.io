    "recipeYield": "8 servings",
    "recipeCategory": "Супи - други",
    "nutrition": {},
    "recipeIngredient": [
      "1 кг лук",
      "1 кг свинско месо",
      "1 морков",
      "1 чушка",
      "2 с.л. доматено пюре",
      "10 г бахар",
      " магданоз",
      " черен пипер",
      " сол",
      " брашно",
