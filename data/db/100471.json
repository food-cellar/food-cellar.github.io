    "name": "Чорба от коприва, сирене и ориз и застройка от кисело мляко, яйце и брашно",
    "image": [],
    "datePublished": "",
    "cookTime": "PT0H30M",
    "recipeYield": "6 servings",
    "recipeCategory": "Супи - други",
    "nutrition": {},
    "recipeIngredient": [
      "300 г коприва",
      "1 морков",
      "2 стръка пресен лук",
      "1 стрък пресен чесън",
      "50 г масло",
      "1 к.ч. ориз",
      "1 с.л. брашно",
      "2 яйца",
      "1 ч.ч. кисело мляко",
      "50 г сирене",
      "2 л. вода"
