    "recipeIngredient": [
      "1/2 ч.ч. червена леща",
      "1 глава лук",
      "2 сладки картофа",
      "3 моркова",
      "1 ябълка",
      "400 г кисело мляко",
      "4 ч.ч. зеленчуков бульон",
      "1/2 с.л. джинджифил",
