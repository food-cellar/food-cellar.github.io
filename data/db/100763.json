    "recipeIngredient": [
      "1/2 кг лапад",
      "1/2 кг коприва",
      "50 г масло",
      "2 с.л. олио",
      "2 моркова",
      "1 глава лук",
      "2 скилидки чесън",
      "2 стръка целина",
      "200 мл. течна сметана",
      "1 1/2 л. вода"
