    "recipeIngredient": [
      "1 кг агнешки дреболии",
      "1 връзка пресен лук",
      "1 к.ч. ориз",
      "2 яйца",
      "1 к.ч. кисело мляко",
      "2 с.л. олио",
      "1 ч.л. червен пипер",
      "1 с.л. оцет",
      " джоджен",
      " магданоз",
