    "nutrition": {},
    "recipeIngredient": [
      "1/2 ч.ч. бял боб",
      "1/2 ч.ч. леща",
      "1/2 кг гъби печурки",
      "1 глава лук",
      "1 морков",
      "1 червена чушка",
      "1 стрък целина",
      "1/2 к.ч. олио",
      "2 домата",
      "2 л. вода",
      " чубрица",
